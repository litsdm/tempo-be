{
  "name": "tempo-be",
  "version": "1.10.1",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "pm2 start dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Carlos Diez <cdiezmoran@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "archiver": "^3.0.0",
    "aws-sdk": "^2.315.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "email-templates": "^5.0.3",
    "expo-server-sdk": "^3.0.1",
    "express": "^4.13.3",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.14",
    "morgan": "^1.8.0",
    "node-cron": "^2.0.1",
    "nodemailer": "^5.1.1",
    "pm2": "^3.5.0",
    "pug": "^2.0.3",
    "resource-router-middleware": "^0.6.0",
    "socket.io": "^2.1.1",
    "stripe": "^6.19.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme",
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
